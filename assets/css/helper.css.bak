


.container{
	max-width: 1100px;
	margin: 0 auto;
}

.primary{
	color: #d9332d;
}

.row, .row::after, .row::before{
	display: table;
	clear: both;
	width: 100%;
	
}

ol, ul{
  list-style: none;
}

a, a:link, a:hover, a:focus, a:active, a:visited{
	text-decoration: none;
	display: inline-block;
}

div[class^="col"]{
	float: left;
	padding: 0 15px;
}

.offset-1{
	margin-left: calc(100% * 1/ 12);
}
.col-4{
width: calc(100% *4/12);
}

.col6{
	width: 50%;
}

.col-7{
	width: calc(100% *7/12);
}
.col10{
	width: calc(100% * 10/12);
}

.w-70{
	width: 70%!;
}
.alignRight{
	text-align:right;
}
.alignCenter{
	text-align: center;
}

.mt-10{
	margin-top: 10px;
}

.mt-20{
	margin-top: 20px;
}
.mt-30{
	margin-top: 30px;
}

.mb-10{
	margin-bottom: 10px;
}
.ml-10{
	margin-left: 10px;
}

.mr-10{
	margin-right: 10px;
}

.pt-10{
	padding-top: 10;
}

.pt-20{
	padding-top: 20;
}

.pr-10{
	padding-right: 10px;
}
.pl-10{
	padding-left: 10px;
}
.pb-10{
	padding-bottom: 10px;
}